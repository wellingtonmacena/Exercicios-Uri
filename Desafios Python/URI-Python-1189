matriz = []
operacao = input().upper()
qtd_valores_adicionados = 0
soma = 0

for volta in range(12):
    vetor = []
    for valor in range(12):
        entrada = float(input())
        vetor.append(entrada)
    matriz.append(vetor)

indice_termino = 1
for vetor in range(1, 11):
    for indice in range(0, indice_termino):
            soma+=matriz[vetor][indice]
            qtd_valores_adicionados+=1

    if vetor <5:
        indice_termino +=1
    if vetor >5:
        indice_termino -= 1

if operacao == "S":
    print("{:.1f}".format(soma))
elif operacao == "M":
    media = soma/qtd_valores_adicionados
    print("{:.1f}".format(media))
