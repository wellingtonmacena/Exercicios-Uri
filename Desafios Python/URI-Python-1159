numero = 1
while numero != 0:
    numero = int(input())
    soma = 0
    volta = 0
    if numero != 0:
        while volta < 5:

            if numero % 2 == 0:
                soma += numero
                numero += 1
                volta += 1
            else:
                numero += 1

        print(soma)
    elif numero == 0:
        break
