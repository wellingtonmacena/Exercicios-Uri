using System;
					
public class Program
{
	public static void Main()
	{
		int i = 1;
		int jota = 7;
		int volta = 1;
		
		while (i <=9)
		{	
			for (int contador = 0; contador <3; contador ++) 
				{
					Console.WriteLine("I=" + i + " J=" + jota);
					jota -=1;
				}
			i +=2;
			
			jota = 7 + ((3 * volta) - volta);
			volta +=1;
		}
	}
}
