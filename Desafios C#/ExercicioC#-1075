using System;

namespace Uri2
{
    class Program
    {
        static void Main(string[] args)
        {
            int entrada = int.Parse(Console.ReadLine());

            for (int contador = 1; contador <= 10000; contador++)
            {
                if (contador % entrada == 2)
                {
                    Console.WriteLine(contador);
                }
            }
            Console.ReadLine();
        }
    }
}
