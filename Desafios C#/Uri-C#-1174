using System;
					
public class Program
{
	public static void Main()
	{
		double[] vetor = new double[100];
		for(int elemento = 0; elemento < vetor.Length; elemento++)
		{	
			vetor[elemento] = double.Parse(Console.ReadLine());
		}
		
		for(int elemento = 0; elemento < vetor.Length; elemento++)
		{
			if (vetor[elemento] <=10)
			{
				Console.WriteLine("A[" + elemento + "] = " + vetor[elemento].ToString("F1"));
			}
		}
	}
}
