{
	public static void Main()
	{
		Console.WriteLine("Matriz");
		int qtd_linhas = int.Parse(Console.ReadLine());
		int qtd_colunas = int.Parse(Console.ReadLine());
		int[,] matriz = new int[qtd_linhas, qtd_colunas];
		
		for (int linhas  = 0; linhas < qtd_linhas; linhas++)
		{
			string[] entrada = Console.ReadLine().Split();
			for (int colunas = 0; colunas < qtd_colunas; colunas++)
			{
				matriz[linhas, colunas]= int.Parse(entrada[colunas]);
			}
		}
		
		for ( int linha = 0; linha < qtd_linhas; linha++)
		{
			for (int coluna = 0; coluna < qtd_colunas; coluna++)
			{
				Console.Write(matriz[linha, coluna] + " ");
			}
			Console.WriteLine();
		}
		
	}
}
